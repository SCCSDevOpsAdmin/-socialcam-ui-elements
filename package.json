{
    "name": "@socialcam/ui-elements",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "npm run clean && npm run build:alias && npm run copy-files && npm run build:sass",
        "build:alias": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
        "build:webpack": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack.config.js",
        "build:dev": "echo 'UI-elements echoing Building Dev...",
        "build:start": "cd dist && PORT=3006 npx serve",
        "build:sass": "sass src/scss:dist/css",
        "start": "echo 'Starting server...'",
        "start:dev": "webpack serve --mode development",
        "start:open": "webpack serve --open --mode development",
        "start:live": "webpack serve --open --mode development --live-reload --hot",
        "watch": "tsc --watch",
        "clean": "rimraf ./dist",
        "copy-files": "copyfiles -E -u 1 src/**/*.scss src/icons/**/* src/scss/**/* dist/ && copyfiles -E -u 1 src/elements/**/*.scss dist/ && copyfiles -E -u 1 src/components/**/*.scss dist/",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check ."
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-flow": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@babel/runtime": "^7.17.2",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.4.1",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-transition-group": "^4.4.4",
        "@types/webpack": "^5.28.0",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "csv-loader": "^3.0.3",
        "dotenv": "^16.0.0",
        "dotenv-webpack": "^8.0.1",
        "fork-ts-checker-webpack-plugin": "^8.0.0",
        "html-webpack-plugin": "^5.5.0",
        "identity-obj-proxy": "^3.0.0",
        "lint-staged": "^13.2.3",
        "mini-css-extract-plugin": "2.7.6",
        "postcss": "^8.4.5",
        "postcss-loader": "^7.3.3",
        "postcss-preset-env": "^9.0.0",
        "prettier": "^3.0.0",
        "react-refresh": "^0.14.0",
        "sass": "^1.48.0",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.1",
        "tsc-alias": "^1.7.1",
        "typescript": "^5.1.6",
        "webpack": "^5.66.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-middleware": "^6.1.1",
        "webpack-dev-server": "^4.7.3",
        "xml-loader": "^1.2.1"
    },
    "dependencies": {
        "@react-google-maps/api": "2.18.1",
        "@types/webpack-env": "^1.17.0",
        "classnames": "^2.3.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-avatar-editor": "^13.0.0",
        "react-dom": "^18.2.0",
        "react-easy-crop": "^5.0.0",
        "react-phone-number-input": "^3.3.0",
        "react-router-dom": "^6.3.0",
        "react-transition-group": "^4.4.2",
        "svg-url-loader": "^8.0.0",
        "use-places-autocomplete": "4.0.0"
    }
}
